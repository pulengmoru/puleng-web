Websitewindow=function(n,t,i,r,u,f,e,o){var s=this;s.drawningboard=n;s.drawningboard.messagemenu=s;s.confirm=function(){s.hide();i&&i()};s.registerevents=function(){r||s.dom.find(".confirm-td").hide();u||s.dom.find(".cancel-td").hide();$("#logobody").append(s.dom);s.dom.find(".confirm").on("click",function(){s.confirm()});s.dom.find(".cancel").on("click",function(){s.hide()});s.dom.find(".close").on("click",function(){s.hide()})};s.show=function(){var n='<div class="freewebsitewrapper"><div class="freewebsitebox"><span class="close">&#215<\/span><div class="message"><div class="boxtitle">'+t+'<\/div><div class="boxtext">'+api.dictionary.preview_website_text+'<\/div><div class="name-field"><div class="error" lang="preview_mail_fname_error"><\/div><\/div><iframe frameBorder="0" width="450" height="300" src="https://www.webself.net/WebSelfAffiliate/BannFreeLogo.aspx?lang='+api.language.toLowerCase()+(e?"&email="+e:"")+(o?"&company="+o:"")+'"><\/iframe><div class="buttombar"><div class="cancel cancel-td">'+u+'<\/div><div class="confirm confirm-td">'+r+"<\/div><\/div><\/div><\/div>";s.dom=$(n);s.registerevents();s.dom.find(".freewebsitebox .message").center();s.dom.find(".freewebsitebox").hide().slideDown("fast",function(){})};s.hide=function(){s.dom.find(".freewebsitebox").slideUp("fast",function(){s.dom.remove()});f&&f();s.drawningboard.messagemenu=undefined}}